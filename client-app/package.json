{
  "name": "hqmx-converter-client",
  "version": "2.0.0",
  "description": "HQMX Converter - 100% Client-side File Converter",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name hqmx-converter",
    "deploy:preview": "npm run build && wrangler pages deploy dist --project-name hqmx-converter --branch preview",
    "lint": "echo 'No linter configured'",
    "typecheck": "echo 'No TypeScript configured'",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@ffmpeg/util": "^0.12.1"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "wrangler": "^3.72.0"
  },
  "keywords": [
    "file-converter",
    "webassembly",
    "client-side",
    "ffmpeg",
    "imagemagick",
    "no-server",
    "privacy-first"
  ],
  "author": "HQMX Team",
  "license": "MIT",
  "homepage": "https://converter.hqmx.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/hqmx/converter.git"
  }
}